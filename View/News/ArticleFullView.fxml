<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<VBox alignment="CENTER" prefWidth="650.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Controller.ArticleFullViewController">

    <ImageView fx:id="image" />
    <Label fx:id="title" styleClass="fv-title-label" wrapText="true">
      <font>
         <Font name="System Bold" size="20.0" />
      </font></Label>
   <Label fx:id="selectArticleL" text="Select an Article">
      <font>
         <Font name="Bookshelf Symbol 7" size="25.0" />
      </font></Label>
    <Label fx:id="publishedAt" alignment="CENTER" prefHeight="7.0" prefWidth="560.0" styleClass="published-at-label">
      <font>
         <Font size="17.0" />
      </font></Label>
    <Label fx:id="source" alignment="CENTER" contentDisplay="CENTER" prefHeight="14.0" prefWidth="611.0" styleClass="source-label">
      <font>
         <Font size="17.0" />
      </font></Label>
    <Label fx:id="author" alignment="CENTER" contentDisplay="CENTER" prefHeight="0.0" prefWidth="536.0" styleClass="author">
      <font>
         <Font size="17.0" />
      </font></Label>
    <TextFlow id="text-flow" prefHeight="476.0" prefWidth="650.0" styleClass="text-flow">
        <Text id="description" fx:id="description" styleClass="description" wrappingWidth="162.0">
         <font>
            <Font name="Droid Sans" size="19.0" />
         </font></Text>
        <Text id="url" fx:id="url" styleClass="url" wrappingWidth="636.0">
         <font>
            <Font size="14.0" />
         </font></Text>
    </TextFlow>
</VBox>
